{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "lib": ["es6"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "outDir": "build",
    "rootDir": "src"
  }
}
